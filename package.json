{
  "name": "aero",
  "displayName": "Aero",
  "icon": "public/icon.png",
  "maintainers": [
    {
      "name": "FrostBreker",
      "url": "https://github.com/FrostBreker/AeroBrowser",
      "email": "frostbreker@github.com"
    }
  ],
  "license": "MIT",
  "version": "0.2.2-alpha",
  "private": true,
  "main": "public/electron.js",
  "homepage": "./",
  "author": {
    "name": "FrostBreker",
    "url": "https://github.com/FrostBreker/AeroBrowser",
    "email": "frostbreker@github.com"
  },
  "description": "Aero is a simple and lightweight web browser for Windows, Mac and Linux. It is built with Electron, React and Redux.",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron . --trace-warnings ",
    "electron:package:mac": "npm run build && electron-builder -m -c.extraMetadata.main=build/electron.js",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
    "electron:package:linux": "npm run build && electron-builder -l -c.extraMetadata.main=build/electron.js",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org fb-projects --project aero ./dist && sentry-cli sourcemaps upload --org fb-projects --project aero ./dist"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version"
    ],
    "development": [
      "last 1 electron version"
    ]
  },
  "fileAssociations": [
    {
      "name": "Document",
      "description": "Aero",
      "role": "Viewer",
      "ext": "html"
    }
  ],
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "electron": "^23.3.13",
    "electron-builder": "^24.13.3",
    "react": "^16.13.1",
    "react-contextmenu": "^2.14.0",
    "react-dom": "^16.13.1",
    "react-redux": "^8.0.5",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.4.2",
    "sass": "^1.58.3"
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.26.12",
    "@sentry/cli": "^2.22.2",
    "@sentry/electron": "^4.14.0",
    "@sentry/tracing": "^7.44.2",
    "dotenv": "^16.3.1",
    "electron-dl": "^3.5.0",
    "electron-is-dev": "^2.0.0",
    "update-electron-app": "^2.0.1"
  }
}
